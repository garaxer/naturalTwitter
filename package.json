{
  "name": "naturaltwitter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "winmon": "set DEBUG=app & nodemon ./bin/www",
    "mon": "DEBUG=app nodemon ./bin/www",
    "lint": "eslint app.js",
    "linter": "./node_modules/.bin/eslint app.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "natural": "^0.6.1",
    "nodemon": "^1.18.4",
    "twitter": "^1.7.1",
    "wordnet-db": "^3.1.11"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garaxer/naturalTwitter.git"
  },
  "author": "Gary, Songhe",
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
